<!DOCTYPE html> 
{% with messages = get_flashed_messages() %}
  {% if messages %}
    <ul class = flashes>
    {% for message in messages %}
      <li>{{ message }}</li>
    {% endfor %}
    </ul>
  {% endif %}
{% endwith %}
<body>
  <form method = "post" enctype = "multipart/form-data">
    <input type = "file" accept = ".json" multiple = "" name = "upload_file">
    <input type = "submit">
  </form>
</body>
